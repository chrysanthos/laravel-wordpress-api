{
    "name": "chrysanthos/laravel-wordpress-api",
    "description": "Simple laravel wrapper for wordpress rest api",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Chrysanthos Prodromou"
        }
    ],
    "require": {
        "php": ">7.3",
        "guzzlehttp/guzzle": "^6.3|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Chrysanthos\\LaravelWordpressApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrysanthos\\LaravelWordpressApi\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chrysanthos\\LaravelWordpressApi\\LaravelWordpressApiServiceProvider"
            ],
            "aliases": {
                "WpApi": "Chrysanthos\\LaravelWordpressApi\\Facades\\WpApi"
            }
        }
    }
}
